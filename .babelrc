{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "import", 
      { "libraryName": "antd-mobile", "style": "css" }
    ], // `style: true` 会加载 less 文件
    // 装饰器所用的插件
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ]
  ]
}