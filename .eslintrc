{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "indent": ["wran", 2],
    "quotes": ["error", "single"],
    "no-console": "off",
    "no-alert": "off",
    "no-unused-vars": "warn"
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "node": true,
    "browser": true
  },
  "overrides": [{
    "files": ["*.json"],
    "rules": {
      "quotes": ["error", "double"]
    }
  }
  ]
}